        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name="com.dcs.dcsbeginneraid.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.dcs.dcsbeginneraid.Weapon" />
        <activity android:name="com.dcs.dcsbeginneraid.Vehicle" />
        <activity android:name="com.dcs.dcsbeginneraid.Helicopter" />
        <activity android:name="com.dcs.dcsbeginneraid.Plane" />
        <activity android:name="com.dcs.dcsbeginneraid.Menu"/>
    </application>

</manifest>